{
  "name": "Huzounet Command Palette",
  "version": "0.1.0",
  "description": "Command palette for Huzounet.fr.",
  "homepage_url": "https://github.com/alexherbo2/huzounet_command_palette",
  "author": "Mathieu Ablasou <alexherbo2@gmail.com>",
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": ["https://huzounet.fr/*"],
      "js": [
        "huzounet_command_palette_module.js"
      ],
      "world": "MAIN"
    }
  ],
  "icons": {
    "16": "assets/huzounet-command-palette-logo@16px.png",
    "32": "assets/huzounet-command-palette-logo@32px.png",
    "48": "assets/huzounet-command-palette-logo@48px.png",
    "128": "assets/huzounet-command-palette-logo@128px.png"
  }
}
